{
  "name": "electron-vite-react",
  "private": true,
  "version": "1.0.0",
  "author": "lichuan",
  "description": "electron demo",
  "keywords": [
    "electron",
    "electron-forge",
    "electron vite"
  ],
  "main": "electronMain/main.js",
  "build": {
    "productName": "electronDemo",
    "nsis": {
      "artifactName": "${productName}.setup.${version}.${ext}",
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true
    },
    "extraResources": [
      "./build/"
    ],
    "publish": [
      {
        "provider": "generic",
        "url": ""
      }
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron:dev": "cross-env NODE_ENV=development electron-forge start",
    "electron:prod": "cross-env NODE_ENV=production electron-forge start",
    "package": "cross-env NODE_ENV=production electron-forge package",
    "make": "cross-env NODE_ENV=production electron-forge make",
    "builderExe": "cross-env NODE_ENV=production electron-builder --win --ia32"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.5",
    "@electron-forge/maker-deb": "^6.0.5",
    "@electron-forge/maker-rpm": "^6.0.5",
    "@electron-forge/maker-squirrel": "^6.0.5",
    "@electron-forge/maker-zip": "^6.0.5",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "electron": "23.1.4",
    "electron-builder": "^23.6.0",
    "typescript": "^4.5.4",
    "vite": "^2.8.0"
  }
}
